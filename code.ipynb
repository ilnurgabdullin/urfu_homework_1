      "source": [
        "def fibonacci(n):\n",
        "    if n < 0:\n",
        "        return -1\n",
        "    if n <= 1:\n",
        "        return n\n",
        "    return fibonacci(n - 1) + fibonacci(n - 2)"
      ]
